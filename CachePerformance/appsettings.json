{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "CacheDbContext": "Server=SHOVON;Database=CacheDbContext;Trusted_Connection=True;MultipleActiveResultSets=true;TrustServerCertificate=True"
  },
  "Hazelcast": {
    "Networking": {
      "Addresses": [ "localhost:5701" ]
    },
    "NearCaches": {
      "students": {
        "InMemoryFormat": "Binary",
        "InvalidateOnChange": true,
        "TimeToLiveSeconds": 0,
        "MaxIdleSeconds": 0,
        "Eviction": {
          "Policy": "Lfu",
          "MaxSizePolicy": "EntryCount",
          "Size": 0
        },
        "CacheLocalEntries": false,
        "LocalUpdatePolicy": "Invalidate",
        "Preloader": {
          "Enabled": true,
          "Directory": "nearcache-students",
          "StoreInitialDelaySeconds": 0,
          "StoreIntervalSeconds": 0
        }
      },
      "subjects": {
        "InMemoryFormat": "Binary",
        "InvalidateOnChange": true,
        "TimeToLiveSeconds": 0,
        "MaxIdleSeconds": 0,
        "Eviction": {
          "Policy": "Lfu",
          "MaxSizePolicy": "EntryCount",
          "Size": 0
        },
        "CacheLocalEntries": false,
        "LocalUpdatePolicy": "Invalidate",
        "Preloader": {
          "Enabled": true,
          "Directory": "nearcache-subjects",
          "StoreInitialDelaySeconds": 0,
          "StoreIntervalSeconds": 0
        }
      },
      "exams": {
        "InMemoryFormat": "Binary",
        "InvalidateOnChange": true,
        "TimeToLiveSeconds": 0,
        "MaxIdleSeconds": 0,
        "Eviction": {
          "Policy": "Lfu",
          "MaxSizePolicy": "EntryCount",
          "Size": 0
        },
        "CacheLocalEntries": false,
        "LocalUpdatePolicy": "Invalidate",
        "Preloader": {
          "Enabled": true,
          "Directory": "nearcache-exams",
          "StoreInitialDelaySeconds": 0,
          "StoreIntervalSeconds": 0
        }
      },
      "marks": {
        "InMemoryFormat": "Binary",
        "InvalidateOnChange": true,
        "TimeToLiveSeconds": 0,
        "MaxIdleSeconds": 0,
        "Eviction": {
          "Policy": "Lfu",
          "MaxSizePolicy": "EntryCount",
          "Size": 0
        },
        "CacheLocalEntries": false,
        "LocalUpdatePolicy": "Invalidate",
        "Preloader": {
          "Enabled": true,
          "Directory": "nearcache-marks",
          "StoreInitialDelaySeconds": 0,
          "StoreIntervalSeconds": 0
        }
      }
    }
  }
}